<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>제어문_반복문</title>


<script>

// 1부터 100까지 합계 10의 배수 출력

let baesu = prompt("숫자 입력");

for(let i = 0; i<=100; i++) {
	let r= i+1;
	if(r % 10 == 0){
		let result = r;
		console.log(baesu+"의 순차합 중 10의 배수 : "+result);
	}
}

let num1=prompt("첫번째 정수 입력(작은수)");
let num2=prompt("두번째 정수 입력(큰수)");

num11 = Number(num1);
num22 = Number(num2);

if (num11>num22){
	console.log("숫자를 다시 입력하세요.");
} else if (num11<num2) { 
	for(let i = 0; i < num22; i++){
		let result = num11 + (num11+1);
	}
	console.log("순차합 : "+result);
	
	
}




</script>

</head>
<body>

</body>
</html>