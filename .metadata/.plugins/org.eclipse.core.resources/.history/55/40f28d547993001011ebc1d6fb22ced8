<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>제어문_반복문</title>


<script>

// 1부터 100까지 합계 10의 배수 출력

let baesu = prompt("숫자 입력");

for(let i = 0; i<=100; i++) {
	let r= i+1;
	if(r % 10 == 0){
		let result = r;
		console.log(baesu+"의 순차합 중 10의 배수 : "+result);
	}
}



let num1 = prompt("첫번쨰 정수 입력");
let num2 = prompt("두번째 정수 입력");

num1 *= 1;
num2 *= 1;

lex max = num1;
let min = num2;

if(num1<num2){
	max = num2;
	min = num1;
}

let sum = 0;
for(let i=min; i<=max; i++){
	sum += i;
	
}

console.log(`${min}부터 ${max}까지의 합계 : ${sum}`);


</script>

</head>
<body>

</body>
</html>